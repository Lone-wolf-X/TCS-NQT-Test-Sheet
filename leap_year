/*
    For leap year conditions--
    1.  year must b mutiple of 400
    2.  year should be multiple of 4 but not multiple of 100.
*/
#include<stdio.h>
int main()
{
    int year = 2016;
    if((year % 400 == 0) || (year % 4 == 0 && year % 100 != 0))
        printf("This is leap year\n");
    else
        printf("This is not leap year\n");
    return 0;
}
